  <%= f.label :first_name, _("First Name") %>
  <%= f.text_field :first_name%>

  <%= f.label :last_name, _("Last Name") %>
  <%= f.text_field :last_name %>

  <%= f.label :nickname, _("Nickname (optional)") %>
  <%= f.text_field :nickname%>

  <%= f.label :birthday, _("Birthday") %>
  <%= f.date_select :birthday, {:prompt => {:day => _('Choose day'), :month => _('Choose month')}, :discard_year => true}, :class =>"accfil accChoo", :style => "width: 181px !important;" %>
  <br class="spacer"/>

  <%= f.label :gender, _("Sex (optional)") %>
  <%= f.select :gender, [[_("Male"),"M"], [_("Female"),"F"]], {:include_blank => true}%>

